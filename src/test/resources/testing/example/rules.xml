<?xml version="1.0"?>
<rules>
    <rule>
        <id>rule_1</id>
        <formula>
            <forall var="v1" in="pat_1">
                <forall var="v2" in="pat_2">
                    <bfunction name="bfunc_1">
                        <param pos="1" var="v1"/>
                        <param pos="2" var="v2"/>
                    </bfunction>
                </forall>
            </forall>
        </formula>
    </rule>

    <rule>
        <id>rule_2</id>
        <formula>
            <exists var="v3" in="pat_3">
                <bfunction name="bfunc_2">
                    <param pos="1" var="v3"/>
                </bfunction>
            </exists>
        </formula>
    </rule>
</rules>
